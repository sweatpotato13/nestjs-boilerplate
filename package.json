{
    "name": "nestjs-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "Cute_Wisp",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier",
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "sdk": "nestia sdk 'src/**/*.controller.ts' --out 'src/api'",
        "swagger": "nestia swagger 'src/**/*.controller.ts' --out './public/swagger.json'",
        "install": "husky init",
        "prepare": "husky"
    },
    "dependencies": {
        "@babel/core": "^7.25.2",
        "@elastic/elasticsearch": "^8.14.0",
        "@nestia/sdk": "^3.9.0",
        "@nestjs/common": "^10.3.10",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.3.10",
        "@nestjs/cqrs": "^10.2.7",
        "@nestjs/elasticsearch": "^10.0.1",
        "@nestjs/microservices": "^10.3.10",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.10",
        "@nestjs/swagger": "^7.4.0",
        "@nestjs/typeorm": "10.0.2",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.4",
        "body-parser": "^1.20.2",
        "bs58": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cls-rtracer": "^2.6.3",
        "cross-fetch": "^4.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.4.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "nest-winston": "^1.10.0",
        "object-assign": "^4.1.1",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.12.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20",
        "typeorm-transactional": "^0.5.0",
        "winston": "^3.13.1",
        "winston-mongodb": "^5.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@eslint/js": "^9.8.0",
        "@nestia/core": "^3.9.0",
        "@nestjs/cli": "^10.4.2",
        "@nestjs/schematics": "^10.1.3",
        "@nestjs/testing": "^10.3.10",
        "@swc/cli": "^0.4.0",
        "@swc/core": "^1.7.3",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4",
        "@types/helmet": "^4.0.0",
        "@types/jest": "29.5.12",
        "@types/node": "^22.0.0",
        "@types/passport": "^1.0.16",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/xml2js": "^0.4.14",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "husky": "^9.1.4",
        "jest": "29.7.0",
        "lint-staged": "^15.2.7",
        "nestia": "^5.5.0",
        "node-mocks-http": "^1.15.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "supertest": "^7.0.0",
        "ts-jest": "29.2.3",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "ttypescript": "^1.5.15",
        "typescript": "5.4.5",
        "typescript-eslint": "8.0.0-alpha.10"
    },
    "peerDependencies": {
        "webpack": "^5.90.3"
    },
    "packageManager": "yarn@4.2.2"
}
