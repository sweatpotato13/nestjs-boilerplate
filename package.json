{
    "name": "nestjs-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "Cute_Wisp",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier",
            "pnpm lint",
            "pnpm prisma:format"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "sdk": "nestia sdk 'src/**/*.controller.ts' --out 'src/api'",
        "swagger": "nestia swagger 'src/**/*.controller.ts' --out './public/swagger.json'",
        "prisma:migrate": "prisma migrate dev",
        "prisma:generate": "prisma generate",
        "prisma:studio": "prisma studio",
        "prisma:format": "prisma format",
        "prepare": "ts-patch install && typia patch && husky"
    },
    "dependencies": {
        "@babel/core": "^7.26.0",
        "@elastic/elasticsearch": "^8.17.0",
        "@nestia/e2e": "^0.7.0",
        "@nestia/fetcher": "^4.5.2",
        "@nestia/sdk": "^4.5.2",
        "@nestjs/common": "^10.4.15",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.15",
        "@nestjs/cqrs": "^10.2.8",
        "@nestjs/elasticsearch": "^10.0.2",
        "@nestjs/microservices": "^10.4.15",
        "@nestjs/mongoose": "^10.1.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.15",
        "@nestjs/swagger": "^8.1.1",
        "@prisma/client": "^6.2.1",
        "body-parser": "^1.20.3",
        "bs58": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cls-rtracer": "^2.6.3",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.2",
        "jsonwebtoken": "^9.0.2",
        "kafkajs": "^2.2.4",
        "moment": "^2.30.1",
        "mongoose": "^8.9.4",
        "morgan": "^1.10.0",
        "nest-winston": "^1.10.0",
        "object-assign": "^4.1.1",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.13.1",
        "prisma": "^6.2.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typia": "^7.5.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/js": "^9.18.0",
        "@nestia/benchmark": "^0.3.0",
        "@nestia/core": "^4.5.2",
        "@nestjs/cli": "^10.4.9",
        "@nestjs/schematics": "^10.2.3",
        "@nestjs/testing": "^10.4.15",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^5.0.0",
        "@types/helmet": "^4.0.0",
        "@types/jest": "29.5.14",
        "@types/node": "^22.10.5",
        "@types/passport": "^1.0.17",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/xml2js": "^0.4.14",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "jest": "29.7.0",
        "lint-staged": "^15.3.0",
        "nestia": "^6.3.1",
        "node-mocks-http": "^1.16.2",
        "prettier": "^3.4.2",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^6.0.1",
        "supertest": "^7.0.0",
        "ts-jest": "29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.3.0",
        "ttypescript": "^1.5.15",
        "typescript": "^5.7.3",
        "typescript-eslint": "8.19.1"
    },
    "peerDependencies": {
        "webpack": "^5.90.3"
    }
}
