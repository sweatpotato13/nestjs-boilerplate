{
    "name": "nestjs-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "Cute_Wisp",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier",
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "types": "ts-node src/common/graphql/generator/type.generator.ts",
        "release": "standard-version",
        "release:major": "standard-version -- --release-as major",
        "release:minor": "standard-version -- --release-as minor",
        "release:patch": "standard-version -- --release-as patch",
        "release:version": "standard-version -- --release-as",
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "sdk": "nestia sdk 'src/**/*.controller.ts' --out 'src/api'",
        "swagger": "nestia swagger 'src/**/*.controller.ts' --out './public/swagger.json'"
    },
    "dependencies": {
        "@babel/core": "^7.21.8",
        "@elastic/elasticsearch": "^8.7.0",
        "@nestjs/common": "^9.4.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.4.0",
        "@nestjs/cqrs": "^9.0.3",
        "@nestjs/elasticsearch": "^9.0.0",
        "@nestjs/microservices": "^9.4.0",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.4.0",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/typeorm": "9.0.1",
        "amqp-connection-manager": "^4.1.13",
        "amqplib": "^0.10.3",
        "body-parser": "^1.20.2",
        "bs58": "^5.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cls-rtracer": "^2.6.2",
        "cross-fetch": "^3.1.5",
        "dotenv": "^16.0.3",
        "express-rate-limit": "^6.7.0",
        "helmet": "^7.0.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.0",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "nest-winston": "^1.9.2",
        "object-assign": "^4.1.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^8.0.0-alpha.7",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.16",
        "typeorm-transactional": "^0.4.1",
        "winston": "^3.8.2",
        "winston-mongodb": "^5.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@nestia/core": "^1.2.2",
        "@nestjs/cli": "^9.4.2",
        "@nestjs/schematics": "^9.1.0",
        "@nestjs/testing": "^9.4.0",
        "@types/helmet": "^4.0.0",
        "@types/jest": "29.5.1",
        "@types/node": "^20.1.2",
        "@types/passport": "^1.0.12",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/xml2js": "^0.4.11",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "git-branch-is": "^4.0.0",
        "husky": "^8.0.3",
        "jest": "29.5.0",
        "lint-staged": "^13.2.2",
        "nestia": "^4.2.0",
        "node-mocks-http": "^1.12.2",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "standard-version": "^9.5.0",
        "supertest": "^6.3.3",
        "ts-jest": "29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.15",
        "typescript": "^5.0.4"
    },
    "packageManager": "yarn@3.4.1"
}
