{
    "name": "nestjs-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "Cute_Wisp",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier",
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "sdk": "nestia sdk 'src/**/*.controller.ts' --out 'src/api'",
        "swagger": "nestia swagger 'src/**/*.controller.ts' --out './public/swagger.json'"
    },
    "dependencies": {
        "@babel/core": "^7.23.0",
        "@elastic/elasticsearch": "^8.10.0",
        "@nestia/sdk": "^2.2.4",
        "@nestjs/common": "^10.2.7",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.2.7",
        "@nestjs/cqrs": "^10.2.6",
        "@nestjs/elasticsearch": "^10.0.1",
        "@nestjs/microservices": "^10.2.7",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.2.7",
        "@nestjs/swagger": "^7.1.13",
        "@nestjs/typeorm": "10.0.0",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.3",
        "body-parser": "^1.20.2",
        "bs58": "^5.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cls-rtracer": "^2.6.3",
        "cross-fetch": "^4.0.0",
        "dotenv": "^16.3.1",
        "express-rate-limit": "^7.1.0",
        "helmet": "^7.0.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "nest-winston": "^1.9.4",
        "object-assign": "^4.1.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^8.0.0-alpha.7",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.17",
        "typeorm-transactional": "^0.4.1",
        "winston": "^3.10.0",
        "winston-mongodb": "^5.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@nestia/core": "^2.1.8",
        "@nestjs/cli": "^10.1.18",
        "@nestjs/schematics": "^10.0.2",
        "@nestjs/testing": "^10.2.7",
        "@types/helmet": "^4.0.0",
        "@types/jest": "29.5.5",
        "@types/node": "^20.8.2",
        "@types/passport": "^1.0.13",
        "@types/passport-google-oauth20": "^2.0.12",
        "@types/xml2js": "^0.4.12",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "husky": "^8.0.3",
        "jest": "29.7.0",
        "lint-staged": "^14.0.1",
        "nestia": "^4.5.0",
        "node-mocks-http": "^1.13.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "supertest": "^6.3.3",
        "ts-jest": "29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.15",
        "typescript": "^5.2.2"
    },
    "packageManager": "yarn@3.4.1"
}
